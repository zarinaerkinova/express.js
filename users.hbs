<h1>Users</h1>

<table>
    <tr>
        <th>Name</th>
        <th>Age</th>
        <th>Id</th>
    </tr>
    {{#each users}}
    <tr>
        <td>{{name}}</td>
        <td>{{age}}</td>
        <td>{{id}}</td>
    </tr>
    {{/each}}
</table>

<form action="/add/user" method="post">
    <input type="text" name="name" id="" placeholder="Name">
    <input type="text" name="age" id="" placeholder="Age">

    <input type="submit">
</form>